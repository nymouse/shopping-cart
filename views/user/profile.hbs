<div class="row justify-content-center">
	<div class="col-md-8 col-md-offset-2">
		<h1>User Profile</h1>
		<hr>
		<h2>My Order</h2>
		{{# each result }}
			<div class="panel panel-defaul">
				<div class="panel-body">
					<ul class="list-group">
					{{# each this.items}}
						<li class="list-group-item">{{this.item.title}} <span class="ml-4">số lượng: {{this.qlt}}</span> <span class="badge badge-info float-right">${{this.price}}</span></li>
					{{/each}}
					</ul>
				</div>
				<div class="panel-footer mb-4 alert alert-success">Total price: ${{this.cart.totalPrice}}</div>
			</div>
		{{/each}}
	</div>
</div>